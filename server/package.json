{
  "name": "snakes-battle-service",
  "version": "1.1.0",
  "description": "Backend service for snakes battle game",
  "main": "src/server.ts",
  "scripts": {
    "start": "npm-run-all --parallel dev:server dev:build",
    "build:prod": "webpack build --progress --color",
    "dev:build": "webpack --watch",
    "dev:server": "webpack && nodemon \"./dist/server.js\" --watch \"./dist\"",
    "preview": "node dist/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"./**/*.+(js|ts)\"",
    "prepare": "cd ../ && husky ./server/.husky",
    "lint-staged": "lint-staged",
    "prisma:push": "prisma db push",
    "prisma:generate": "prisma generate"
  },
  "lint-staged": {
    "*.{js, ts}": [
      "eslint --quiet --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "keywords": [],
  "author": "DraguaStudio",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-validator": "^7.2.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "socket.io": "^4.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@prisma/adapter-pg": "^5.18.0",
    "@prisma/client": "^5.18.0",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^22.4.1",
    "@types/pg": "^8.11.6",
    "@types/webpack-node-externals": "^3.0.4",
    "@typescript-eslint/parser": "^8.2.0",
    "clean-webpack-plugin": "^4.0.0",
    "commitlint": "^19.4.0",
    "compression-webpack-plugin": "^11.1.0",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.1.5",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.9",
    "nodemon": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
